{
    "manifest_version": 2,
    "name": "test tool",
    "version": "1.0",
    "description": "测试工具",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": [
            "http://*/*"
        ],
        "js": [
            "loader.js"
        ],
        "run_at": "document_idle"
    }]
}